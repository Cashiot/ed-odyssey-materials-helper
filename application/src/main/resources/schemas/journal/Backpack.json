{
  "$schema" : "https://json-schema.org/draft/2020-12/schema",
  "type" : "object",
  "description": "Lists the contents of the backpack, in a separate backpack.json file",
  "extends" : { "$ref": "Event.json" },
  "properties" : {
    "Items" : {
      "title" : "Items",
      "type" : "array",
      "items" : {
        "type" : "object",
        "properties" : {
          "Name" : {
            "title" : "Name",
            "type" : "string",
            "examples" : [ "suitschematic", "ionisedgas", "buildingschematic" ]
          },
          "Name_Localised" : {
            "title" : "Name_Localised",
            "type" : "string",
            "examples" : [ "Suit Schematic", "Ionised Gas", "Building Schematic" ]
          },
          "OwnerID" : {
            "title" : "OwnerID",
            "type" : "integer",
            "examples" : [ 2954264951, 2953652017, 2956046928 ]
          },
          "Count" : {
            "title" : "Count",
            "type" : "integer",
            "examples" : [ 1, 4, 2 ]
          },
          "MissionID" : {
            "title" : "MissionID",
            "type" : "integer",
            "examples" : [ 895446120, 895536604, 895547209 ]
          }
        },
        "additionalProperties" : false,
        "required" : [ "Name", "OwnerID", "Count" ]
      }
    },
    "Components" : {
      "title" : "Components",
      "type" : "array",
      "items" : {
        "type" : "object",
        "properties" : {
          "Name" : {
            "title" : "Name",
            "type" : "string",
            "examples" : [ "chemicalcatalyst", "circuitswitch", "electricalfuse" ]
          },
          "Name_Localised" : {
            "title" : "Name_Localised",
            "type" : "string",
            "examples" : [ "Chemical Catalyst", "Circuit Switch", "Electrical Fuse" ]
          },
          "OwnerID" : {
            "title" : "OwnerID",
            "type" : "integer",
            "examples" : [ 0 ]
          },
          "Count" : {
            "title" : "Count",
            "type" : "integer",
            "examples" : [ 1, 3, 9 ]
          },
          "MissionID" : {
            "title" : "MissionID",
            "type" : "integer",
            "examples" : [ 796428510 ]
          }
        },
        "additionalProperties" : false,
        "required" : [ "Name", "OwnerID", "Count" ]
      }
    },
    "Consumables" : {
      "title" : "Consumables",
      "type" : "array",
      "items" : {
        "type" : "object",
        "properties" : {
          "Name" : {
            "title" : "Name",
            "type" : "string",
            "examples" : [ "healthpack", "energycell", "amm_grenade_emp" ]
          },
          "Name_Localised" : {
            "title" : "Name_Localised",
            "type" : "string",
            "examples" : [ "Medkit", "Energy Cell", "Shield Disruptor" ]
          },
          "OwnerID" : {
            "title" : "OwnerID",
            "type" : "integer",
            "examples" : [ 0 ]
          },
          "Count" : {
            "title" : "Count",
            "type" : "integer",
            "examples" : [ 1, 2, 3 ]
          }
        },
        "additionalProperties" : false,
        "required" : [ "Name", "Name_Localised", "OwnerID", "Count" ]
      }
    },
    "Data" : {
      "title" : "Data",
      "type" : "array",
      "items" : {
        "type" : "object",
        "properties" : {
          "Name" : {
            "title" : "Name",
            "type" : "string",
            "examples" : [ "bloodtestresults", "evacuationprotocols", "radioactivitydata" ]
          },
          "Name_Localised" : {
            "title" : "Name_Localised",
            "type" : "string",
            "examples" : [ "Blood Test Results", "Evacuation Protocols", "Radioactivity Data" ]
          },
          "OwnerID" : {
            "title" : "OwnerID",
            "type" : "integer",
            "examples" : [ 0, 9641473884067, 125843126 ]
          },
          "Count" : {
            "title" : "Count",
            "type" : "integer",
            "examples" : [ 1, 2, 3 ]
          },
          "MissionID" : {
            "title" : "MissionID",
            "type" : "integer",
            "examples" : [ 895547126, 895609165, 897631120 ]
          }
        },
        "additionalProperties" : false,
        "required" : [ "Name", "OwnerID", "Count" ]
      }
    }
  },
  "additionalProperties" : false,
  "required" : [ "Items", "Components", "Consumables", "Data" ]
}